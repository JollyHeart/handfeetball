{
  "name": "handfeetball",
  "version": "1.0.0",
  "description": "Handfeetball official website built with Eleventy and Netlify CMS",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "dev": "npm-run-all --parallel start css:watch",
    "css:build": "postcss src/styles/tailwind.css -o dist/styles.css",
    "css:watch": "postcss src/styles/tailwind.css -o dist/styles.css --watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "postcss-cli": "^10.1.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "netlify-cms-app": "^2.15.72" 
  }
}

